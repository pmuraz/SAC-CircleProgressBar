{
    "id": "com-pmuraz-sap-circleprogressbarwidget",
    "version": "1.0.2",
    "name": "Circle Progress Bar Widget",
    "description": "SAC Custom Widget to add a beautiful and animated circle progress bar in SAP Analytics Cloud",
    "newInstancePrefix": "CircleProgressBarWidget",
    "vendor": "pmuraz",
    "eula": "",
    "license": "MIT",
    "icon": "https://pmuraz.github.io/SAC-CircleProgressBar/icon.png",
    "webcomponents": [
        {
            "kind": "main",
            "tag": "com-pmuraz-sap-circleprogressbarwidget",
            "url": "https://pmuraz.github.io/SAC-CircleProgressBar/CircleProgressBarWidget.js",
            "integrity": "",
            "ignoreIntegrity": true
        },
        {
            "kind": "builder",
            "tag": "com-pmuraz-sap-circleprogressbarwidget-builder",
            "url": "https://pmuraz.github.io/SAC-CircleProgressBar/CircleProgressBarWidget_Builder.js",
            "integrity": "",
            "ignoreIntegrity": true
        }
    ],
    "properties": {
        "percentage": {
            "description": "Percentage (Manual Input)",
            "type": "integer",
            "default": 75
        },
        "barColor": {
            "description": "Bar Color",
            "type": "string",
            "default": "#03ff4f"
        },
        "emptyBarColor": {
            "description": "Empty Bar Color",
            "type": "string",
            "default": "#ededed"
        }
    },
    "methods": {
        "setPercentage": {
            "description": "Set Percentage",
            "parameters": [
                {
                    "name": "percentage",
                    "type": "integer",
                    "description": "Percentage"
                }
            ],
            "body": "this.percentage = percentage;"
        },
        "getPercentage": {
            "returnType": "integer",
            "description": "Return Percentage",
            "body": "return this.percentage;"
        }
    },
    "events": {
        "onClick": {
            "description": "User Clicked."
        }
    }
}
